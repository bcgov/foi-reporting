                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @runcycleid integer 
SET @runcycleid = ?

Select 
RFD.iActionID as actionid,
RFD.iRequestID as foirequestid,
@runcycleid as runcycleid,
RFD.siActionType as actiontype,
RFD.vcDescription as description,
P.vcName as priority,
RFD.vcEmailAddresses as emailaddress,
RFD.dtCreateDate as createddate,
RFD.sdtActionDate as actiondate,
RFD.sdtDueDate as duedate,
RFD.sdtResponseDate as responsedate,
RFD.iParentActionID as parentactionid,
[dbo].AFX_ProperName(TU.FIRST_NAME,TU.LAST_NAME) as createdby,
RFD.vcSubject as subject,
RFD.tiProgramOfficeID as programofficeid,
RFD.tiReqForDocStatus as reqfordocstatusid,
RFD.dtCompletedDate as completeddate,
TR.tiOfficeID as requestofficeid,
TR.vcVisibleRequestID as visiblerequestid,
TR.vcDescription as requestdescription,
isnull((select TPO.tiOfficeID from tblProgramOffices TPO where TPO.tiProgramOfficeID = RFD.tiProgramOfficeID),TR.tiOfficeID) as officeid,
TR.tiRequestTypeID as requesttypeid,
isnull([dbo].AFX_GetRequestAge(RFD.sdtDueDate,getdate(),isnull((select TPO.tiOfficeID from tblProgramOffices TPO where TPO.tiProgramOfficeID = RFD.tiProgramOfficeID),TR.tiOfficeID)), 0) as overduedays,
isnull([dbo].AFX_GetRequestAge(RFD.sdtActionDate,RFD.dtCompletedDate,isnull((select TPO.tiOfficeID from tblProgramOffices TPO where TPO.tiProgramOfficeID = RFD.tiProgramOfficeID),TR.tiOfficeID)), 0) as elapseddays,
case when RFD.dtCompletedDate&gt;RFD.sdtDueDate then [dbo].AFX_GetRequestAge(RFD.sdtDueDate,RFD.dtCompletedDate, isnull((select TPO.tiOfficeID from tblProgramOffices TPO where TPO.tiProgramOfficeID = RFD.tiProgramOfficeID),TR.tiOfficeID)) else 0 end as passduedays,
isnull([dbo].AFX_GetRequestAge(RFD.sdtActionDate,isnull(RFD.dtCompletedDate,getdate()), isnull((select TPO.tiOfficeID from tblProgramOffices TPO where TPO.tiProgramOfficeID = RFD.tiProgramOfficeID),TR.tiOfficeID)), 0) as rfdage,
case when RFD.sdtDueDate&gt;getdate() then isnull([dbo].AFX_GetRequestAge(getdate(),RFD.sdtDueDate,isnull((select TPO.tiOfficeID from tblProgramOffices TPO where TPO.tiProgramOfficeID = RFD.tiProgramOfficeID),TR.tiOfficeID)), 0) else 0 end as remainingdays,
RFD.METHOD_OF_DELIVERY as methodofdelivery,
'Y' as activeflag
From [dbo].[tblRequestForDocuments] RFD 
left outer join [dbo].[viwPriorities] P on RFD.siPriority = P.tiPriority and tiLocaleID = 1 
left outer join EC_USERS tu on RFD.iCreatedBy = tu.USER_ID 
left outer join [dbo].[tblRequests] TR ON RFD.iRequestID = TR.iRequestID</property>
