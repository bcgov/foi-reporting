                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @currentruntimeidloadid integer 
SET @currentruntimeidloadid = ?

Select 
@currentruntimeidloadid as runcycleid,
R.iRequestID as foirequestid,
R.tiRequestTypeID as requesttypeid,
R.iRequestStatusID as requeststatusid,
R.tiReceivedType as receivedmodeid,
R.tiDeliveryType as deliverymodeid,
R.tiRequesterCategoryID as applicantcategoryid,
R.sdtRequestedDate as requesteddate,
R.sdtReceivedDate as receiveddate,
R.sdtReceivedDate as startdate,
R.sdtCreatedDate as createddate,
R.sdtModifiedDate as modifieddate,
R.sdtAssignedDate as assigneddate,
LTRIM(RTRIM(RBU.LAST_NAME)) + ', ' + LTRIM(RTRIM(RBU.FIRST_NAME)) as receivedbyusername,
LTRIM(RTRIM(MBU.LAST_NAME)) + ', ' + LTRIM(RTRIM(MBU.FIRST_NAME)) as modifiedbyusername,
R.iAssignedBy as assignedbyid,
R.iAssignedTo as assignedtoid,
AT.FULL_NAME as primaryusername,
AT.DEFAULT_GROUP_ID as primarygroupid,
isnull(dbo.cst_GetGroupForOfcReqType(VO.OFFICE_CODE, TLL.vcTerminology), '') as groupname,
R.tiOfficeID as officeid,
dbo.cst_GetMinistryInfo ('Name', R.tiOfficeID) as ministry,
ISNULL(dbo.fn_StripCharacters(dbo.cst_GetRqstCustFieldValue (R.iRequestID, 'Subject'), '^a-z0-9 '),'') as subject,
R.sdtScreenedDate as screeneddate,
R.sdtTargetDate as targetdate,
R.cAmendment as amendment,
isnull(R.sdtAmendmentCreatedDate, '') as amendmentcreateddate,
R.iAmendmentCreatedBy as amendmentcreatedby,
isnull(R.sdtCompletedDate, '') as completeddate,
R.iCompletedBy as completedby,
isnull(R.sdtDeliveryDate, '') as deliverydate,
R.iDeliveredBy as deliveredby,
isnull(R.sdtClosedDate, '') as closeddate,
R.iClosedBy as closedby,
isnull(R.vcNotes, '') as notes,
isnull(R.vcWithheldStage, '') as withheldstage,
isnull(R.vcOtherAddress, '') as otheraddress,
isnull(R.vcHoldStage, '') as holdstage,
isnull(R.sdtHoldDate, '') as holddate,
R.tiAppealType as appealtype,
R.tiReviewID as reviewid,
isnull(R.sdtWithheldDate, '') as withhelddate,
ISNULL(term.vcTerminology,'') as disposition,
ISNULL(dbo.cst_GetRqstStatusDescription (R.vcRequestStatus), '') + ISNULL(CASE R.vcRequestStatus  WHEN 'Closed' THEN  CASE WHEN term.vcTerminology IS NULL THEN  NULL  ELSE ' - '+ISNULL(term.vcTerminology, '') END  ELSE NULL END, '') as execcomments,
R.sdtOriginalTargetDate as originaltargetdate,
isnull(RC.vcDescription, '') as redactiondescription,
ISNULL(tsk.vcDescription,'') as currentactivity,
ISNULL([dbo].AFX_GetHoldCount(CASE Convert(int, [dbo].AFX_GetConfigValue ('Case','TDSCriteria')) WHEN 0 THEN R.sdtReceivedDate WHEN 1 THEN R.sdtAssignedDate ELSE R.sdtScreenedDate END, getdate(),  R.iRequestID, R.tiOfficeID), 0) as onholddays,
R.iProcessedDays as processeddays,
isnull(VRF.vcName, '') as releaseformat,
isnull(VDA.vcName, '') as denialauthority,
isnull(LTRIM(RTRIM(CW.LAST_NAME)) + ', ' + LTRIM(RTRIM(CW.FIRST_NAME)), '') as caseowner,
isnull(CW.TITLE, '') as caseownertitle,
CW.EMAILID as caseowneremail,
isnull(CW.WORK_PHONE, '') as caseownerphone,
isnull(R.sdtOriginalClosedDate, '') as originalcloseddate,
isnull(R.sdtDueDate, '') as stdduedate,
R.iRemainingDays as remainingdays,
R.iRequestAge as requestage,
isnull((SELECT MAX(CAST(sdtActionDate AS DATE)) as Current_Activity_date FROM dbo.tblHistoryEvents hev WHERE hev.iRequestID = R.iRequestID AND hev.tiReviewID = R.tiReviewID), '')  as currentactivitydate,
ISNULL(dbo.cst_GetRqstCustFieldValue (R.iRequestID, 'XGOV'),'') as crossgovtno,
ISNULL(dbo.fn_StripCharacters(R.vcDescription, '^a-z0-9 '),'') as requestdescription,
R.vcRequestStatus as requeststatus,
dbo.cst_GetRqstStatusDescription (r.vcRequestStatus) as status,
CASE WHEN R.vcRequestStatus = 'Closed' THEN CASE WHEN CASE WHEN R.sdtClosedDate &gt; R.sdtTargetDate THEN isnull([dbo].AFX_GetRequestAge(R.sdtTargetDate, R.sdtClosedDate, R.tiOfficeID), 0)  - isnull([dbo].AFX_GetHoldCount(R.sdtTargetDate, R.sdtClosedDate, R.iRequestID, R.tiOfficeID), 0) ELSE 0 END &gt; 0 THEN 0 ELSE 1 END ELSE CASE WHEN dbo.cst_GetRqstStatusDescription (R.vcRequestStatus) LIKE 'On Hold%' THEN 1 WHEN (ISNULL (dbo.AFX_GetRequestAge (R.sdtTargetDate, CONVERT(DATETIME, CONVERT(DATE, GETDATE())), R.tiOfficeID), 0) - ISNULL (dbo.AFX_GetHoldCount (R.sdtTargetDate, CONVERT(DATETIME, CONVERT(DATE, GETDATE())), R.iRequestID, R.tiOfficeID), 0)) &gt; 0 THEN 0 ELSE 1 END END as countontime,
CASE WHEN R.vcRequestStatus = 'Closed' THEN CASE WHEN CASE WHEN R.sdtClosedDate &gt; R.sdtTargetDate THEN isnull([dbo].AFX_GetRequestAge(R.sdtTargetDate, R.sdtClosedDate, R.tiOfficeID), 0)  - isnull([dbo].AFX_GetHoldCount(R.sdtTargetDate, R.sdtClosedDate, R.iRequestID, R.tiOfficeID), 0) ELSE 0 END &gt; 0 THEN 1 ELSE 0 END ELSE CASE WHEN dbo.cst_GetRqstStatusDescription (R.vcRequestStatus) LIKE 'On Hold%' THEN 0 WHEN (ISNULL (dbo.AFX_GetRequestAge (R.sdtTargetDate, CONVERT(DATETIME, CONVERT(DATE, GETDATE())), R.tiOfficeID), 0) - ISNULL (dbo.AFX_GetHoldCount (R.sdtTargetDate, CONVERT(DATETIME, CONVERT(DATE, GETDATE())), R.iRequestID, r.tiOfficeID), 0)) &gt; 0 THEN 1 ELSE 0 END END as countoverdue,
CASE WHEN R.vcRequestStatus = 'Closed' and R.sdtClosedDate &gt; R.sdtTargetDate THEN ISNULL (dbo.AFX_GetRequestAge (R.sdtTargetDate, R.sdtClosedDate, R.tiOfficeID), 0) - ISNULL (dbo.AFX_GetHoldCount (R.sdtTargetDate, R.sdtClosedDate, R.iRequestID, R.tiOfficeID), 0) WHEN R.vcRequestStatus = 'Closed' and R.sdtClosedDate &lt;= R.sdtTargetDate THEN 0 WHEN dbo.cst_GetRqstStatusDescription (R.vcRequestStatus) LIKE 'On Hold%'  THEN 0 WHEN CONVERT(DATETIME, CONVERT(DATE,GETDATE())) &lt;= R.sdtTargetDate THEN 0 ELSE ISNULL (dbo.AFX_GetRequestAge (R.sdtTargetDate, CONVERT(DATETIME, CONVERT(DATE, GETDATE())), R.tiOfficeID), 0) - ISNULL (dbo.AFX_GetHoldCount (R.sdtTargetDate, CONVERT(DATETIME, CONVERT(DATE, GETDATE())), R.iRequestID, R.tiOfficeID), 0) END as daysoverdue,
ISNULL(dbo.fn_StripCharacters(dbo.cst_GetRqstCustFieldValue (R.iRequestID, 'Publication Status'), '^a-z0-9 '),'') as publication,
ISNULL(dbo.fn_StripCharacters(dbo.cst_GetRqstCustFieldValue (R.iRequestID, 'Do Not Publish Reason'), '^a-z0-9 '),'') as publicationreason,
isnull([dbo].[cst_GetRqstCustFieldValue](R.iRequestID, 'OIPC No'), '') as oipcno,
isnull([dbo].[cst_GetRqstCustFieldValue](R.iRequestID, 'Judicial Review'), '') as judicialreview,
isnull([dbo].[cst_GetRqstCustFieldValue](R.iRequestID, 'Review Type'), '') as reviewtype,
isnull([dbo].[cst_GetRqstCustFieldValue](R.iRequestID, 'Reason'), '') as reason,
isnull([dbo].[cst_GetRqstCustFieldValue](R.iRequestID, 'Portfolio Officer'), '') as portfolioofficer,
CASE WHEN R.sdtClosedDate &gt; R.sdtTargetDate THEN isnull([dbo].AFX_GetRequestAge(R.sdtTargetDate, R.sdtClosedDate, R.tiOfficeID), 0)  - isnull([dbo].AFX_GetHoldCount(R.sdtTargetDate, R.sdtClosedDate, R.iRequestID, R.tiOfficeID), 0) ELSE 0 END as passduedays,
R.tiExtension as extension, 
R.vcDescription as description,
P.vcName as priority,
R.iRequesterID as requesterid,
R.iOnBehalfOf as onbehalfofrequesterid,
R.ireferencenumber as referencenumber,
isnull(substring([dbo].[AFX_GetReferenceRequests](r.iRequestID, 'R'), 1, 50),'') as refvisualrequestfilenumber,
R.iShipAddressID as shipaddressid,
R.iBillAddressID as billaddressid,
R.sdtOriginalReceivedDate as originalreceiveddate,
[dbo].[cst_GetRqstCustFieldValue](R.iRequestID, 'Coordinated NR Response Req''d') as coordinatednrresponsereqd,
[dbo].[cst_GetRqstCustFieldValue](R.iRequestID, 'Applicant''s File Reference') as applicantfilereference,
isnull((Select secondaryusers from (Select RAH.iRequestID, STRING_AGG(STCU.FULL_NAME, ':: ') as secondaryusers from [dbo].[tblRequestsAssignHistory] RAH left outer join [dbo].EC_USERS AS STCU ON RAH.iUserID = STCU.USER_ID group by RAH.iRequestID) su WHERE su.iRequestID = R.iRequestID), '')  as secondaryusers,
isnull((Select noofdocdelivered from (Select iRequestID, Count(iDeliveryID) as noofdocdelivered from [dbo].[tblDeliveryLog] where tiStatus = 1 group by iRequestID) dl WHERE dl.iRequestID = R.iRequestID), '') noofdocdelivered,  
'Y' as activeflag 
From [dbo].tblRequests R 
left outer join dbo.tblTasks tsk ON R.tiReviewID = tsk.tiTaskID
left outer join [dbo].EC_USERS AS RBU ON R.iReceivedBy = RBU.USER_ID 
left outer join [dbo].EC_USERS AS MBU ON R.iModifiedBy = MBU.USER_ID 
left outer join [dbo].EC_USERS AS AT ON R.iAssignedTo = AT.USER_ID 
left outer join [dbo].EC_USERS AS CW ON R.iCaseOwner = CW.USER_ID 
left outer join [dbo].tblDispositions disp on R.vcFinalDisposition = disp.vcDispositionKey 
left outer join [dbo].tblTerminologyLookup term ON disp.iLabelID = term.iLabelID and term.tiLocaleID = 1 
left outer join [dbo].viwRedactionCodes RC on R.siRedactionCodeID = RC.siRedactionCodeID and RC.tiLocaleID = 1 
left outer join [dbo].viwReleaseFormats VRF on R.siReleaseFormatID = VRF.siReleaseFormatID and VRF.tiLocaleID = 1 
left outer join [dbo].viwDenialAuthorities VDA on R.siDelAuthorityID = VDA.siCodeID and VDA.tiLocaleID = 1
left outer join [dbo].tblRequestTypes RT on R.tiRequestTypeID = RT.tiRequestTypeID 
left outer join [dbo].tblTerminologyLookup TLL ON RT.iLabelID = TLL.iLabelID and TLL.tiLocaleID = 1 
left outer join [dbo].EC_VW_OFFICE VO on R.tiOfficeID = VO.OFFICE_ID AND VO.LOCALE_ID = 1 
left outer join [dbo].[viwPriorities] P on R.tiPriority = P.tiPriority and P.tiLocaleID = 1</property>
