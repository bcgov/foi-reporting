                          dataType="System.String"
                          description="The SQL command to be executed."
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @currentruntimeidloadid integer 
SET @currentruntimeidloadid = ?

Select 
@currentruntimeidloadid as runcycleid,
R.iRequestID as foirequestid,
R.tiRequestTypeID as requesttypeid,
R.iRequestStatusID as requeststatusid,
R.tiReceivedType as receivedmodeid,
