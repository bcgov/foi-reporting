                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Select 
	iRequestID as foirequestid,
	CAST(? as INT) as runcycleid,
	tiPaymentMode as paymentmodeid,
	nEstimatedAmount as estimatedamount,
	nIncurredAmount as incurredamount,
	nInterestAmount as interestamount,
