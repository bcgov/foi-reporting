	sdtInvoicedDate as invoicedate,
	nFeeWaived as feewaived,
	ISNULL((Select SUM(ISNULL(nChargedAmount, 0)) from tblInvoices where TR.iRequestID = iRequestID AND cInvoiceNumber &lt;&gt; '0' GROUP By iRequestID), 0) as totalinvoicedamount,
	[dbo].AFX_GetRequestAmount(TR.iRequestID, 'PREPAYMENTAMOUNT', GETDATE()) as prepaymentamount,
	cApplicationFee as applicationfeeflag,
	[dbo].AFX_GetRequestAmount(TR.iRequestID, 'REFUNDAMOUNT', GETDATE()) as refundamount,
	[dbo].AFX_GetRequestAmount(TR.iRequestID, 'AMOUNTPAID',getdate()) as feepaidamount,
	vcPaymentStatus as paymentstatus,
	cCostEstimated as costestimated,
	COALESCE(isnull((Select sdtPaymentReceivedDate from (Select t.iRequestID, t.cInvoiceNumber , max(t.sdtPaymentReceivedDate) as sdtPaymentReceivedDate from [dbo].[tblTransactions] t group by t.iRequestID, t.cInvoiceNumber) prd WHERE prd.iRequestID = TR.iRequestID and prd.cInvoiceNumber = TR.cInvoiceNumber), ''), NULL) as paymentreceiveddate,
